# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_BASE_URL=https://api.elevenlabs.io
ELEVENLABS_OUTBOUND_CALLS_PATH=/v1/convai/twilio/outbound-call
ELEVENLABS_SIP_URI=el-sbc.example.com;transport=tls

# Redis Configuration (Railway Redis)
REDIS_URL=your_railway_redis_url_here

# Database Configuration (Supabase)
DATABASE_URL=your_supabase_database_url_here

# Server Configuration
SERVICE_PORT=3000
CALLS_WORKER_CONCURRENCY=10
RETENTION_DAYS=15
LOG_LEVEL=info
PUBLIC_BASE_URL=https://voice.ateneai.com
PUBLIC_WS_STREAM_URL=wss://your-domain/ws/stream

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_FROM_FALLBACK=+1234567890
